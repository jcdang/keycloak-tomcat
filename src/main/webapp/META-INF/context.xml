<?xml version="1.0" encoding="UTF-8"?>
<Context path="/auth">
  <Resource name="KeycloakDS" auth="Container" type="javax.sql.DataSource"
            maxTotal="100" maxIdle="30" maxWaitMillis="10000"
            username="sa" password="sa" driverClassName="org.h2.Driver"
            url="jdbc:h2:file:${catalina.base}/data/auth;AUTO_SERVER=TRUE"/>
</Context>